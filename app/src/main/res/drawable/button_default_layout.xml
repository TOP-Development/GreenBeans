<selector xmlns:android="http://schemas.android.com/apk/res/android">

<item android:state_pressed="false">
    <shape android:shape="rectangle">
        <!--<stroke android:color="@color/greenbeans_secondary" android:width="@dimen/ui_stroke_width"/>-->
        <solid android:color="@color/greenbeans_primary"/>
        <corners android:radius="10dp"/>
    </shape>
</item>

<item android:state_pressed="true">
    <shape android:shape="rectangle">
        <!--<stroke android:color="@color/greenbeans_secondary" android:width="@dimen/ui_stroke_width"/>-->
        <solid android:color="@color/greenbeans_primary_dark"/>
        <corners android:radius="10dp"/>
    </shape>
</item>

</selector>